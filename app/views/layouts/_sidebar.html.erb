          </ul>
          <ul class="metismenu" id="menu">
            <li class="<%= add_class_active('admin/users') %>">
              <a href="javascript:void(0)" data-toggle="collapse" data-target="#sidebar-users"><%= t('label.sidebar.users') %></a>
              <ul class="collapse <%= display_submenu('admin/users') %> " id="sidebar-users">
                <li class="<%= add_active_sub_menu('admin/users') %>">
                  <%= link_to "Index", admin_users_path %>
                </li>
              </ul>
            </li>
          </ul>
